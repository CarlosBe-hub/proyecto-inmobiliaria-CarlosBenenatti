@model ProyectoInmobiliaria.Models.Propietario

@{
    ViewData["Title"] = "Detalles Propietario";
}

<h2>Detalles</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.Nombre</dd>
        <dt class="col-sm-2">Apellido</dt>
        <dd class="col-sm-10">@Model.Apellido</dd>
        <dt class="col-sm-2">DNI</dt>
        <dd class="col-sm-10">@Model.Documento</dd>
    </dl>
</div>

<p>
    <a asp-action="Edit" asp-route-id="@Model.IdPropietario" class="btn btn-warning">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver</a>
</p>
